{
    "manifest_version": 3,
    "version": "1.00",
    "name": "QC-Helper",
    "description": "Helper for Gitlab and Teamwork",
    "action": {
        "default_icon": {
            "16": "/img/16.png",
            "32": "/img/32.png",
            "64": "/img/64.png",
            "128": "/img/128.png"
        }
    },
    "icons": {
        "16": "/img/16.png",
        "32": "/img/32.png",
        "64": "/img/64.png",
        "128": "/img/128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://gitlab.com/*"],
            "css": ["style.css"],
            "js": ["gitlabHelper.js"]
        }
    ]
}
